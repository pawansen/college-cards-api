{
  "name": "vaow-backend",
  "version": "1.0.0",
  "description": "vaow-backend",
  "main": "index.js",
  "scripts": {
    "migrate": "node src/api/domain/migrations",
    "seeder": "node src/api/domain/seeders",
    "rm:logs": "node src/api/utils/removeLogFiles",
    "rm:upload": "node src/api/utils/removeUploadFiles",
    "rm:files": "npm run rm:logs && npm run rm:upload",
    "db:migrate": "npm run migrate && npm run seeder",
    "start": "node index.js",
    "start:dev": "nodemon",
    "dev": "pm2 start --name vaow-backend-dev index.js",
    "logs": "pm2 log vaow-backend-dev",
    "flush": "pm2 flush vaow-backend-dev",
    "dev:restart": "pm2 restart vaow-backend-dev",
    "dev:delete": "pm2 delete vaow-backend-dev",
    "dev:start": "npm run dev && npm run logs",
    "dev:flush": "npm run flush && npm run dev:restart && npm run logs",
    "prettier": "prettier \"src/**/*.{js,json,cjs,ejs}\" --write",
    "lint": "eslint index.js ./src",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "VAOW",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.40.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "express": "^4.17.2",
    "jest": "^29.7.0",
    "mongodb": "^6.8.0",
    "mysql-import": "^5.0.26",
    "mysql2": "^3.1.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.6.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "dependencies": {
    "axios": "^1.6.3",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "custom-env": "^2.0.1",
    "dotenv": "^16.0.0",
    "express-validator": "^6.14.0",
    "fast-csv": "^5.0.1",
    "get-current-line": "^7.4.0",
    "http-errors": "^2.0.0",
    "ip": "^1.1.8",
    "joi": "^17.11.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.30.1",
    "mongoose": "^8.2.1",
    "morgan": "^1.10.0",
    "morgan-json": "^1.1.0",
    "mqtt": "^5.6.1",
    "multer": "^1.4.5-lts.1",
    "oauth2-server": "^3.1.1",
    "path": "^0.12.7",
    "redis": "^3.1.2",
    "request-ip": "^3.3.0",
    "swagger-cli": "^4.0.4",
    "swagger-ui-express": "^4.6.2",
    "winston": "^3.5.1",
    "yup": "^1.0.0"
  }
}
